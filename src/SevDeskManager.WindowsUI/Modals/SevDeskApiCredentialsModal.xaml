<Window x:Class="SERGO.SevDeskManager.WindowsUI.Modals.SevDeskApiCredentialsModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SERGO.SevDeskManager.WindowsUI.Modals"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="SevDesk API Key" Height="150" Width="300">
    <Grid Margin="0,0,0,-6">
        <Button Content="Speichern" HorizontalAlignment="Center" Margin="0,70,0,0" VerticalAlignment="Top" Width="210" Click="ButtonClick_SaveApiKey"/>
        <TextBox Name="TextBoxSevDeskApiKey" HorizontalAlignment="Center" Margin="0,31,0,0" Text="" TextWrapping="Wrap" Width="210" Height="34" VerticalAlignment="Top"/>
        <Label Content="SevDesk API Key" HorizontalAlignment="Center" Margin="0,5,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
